PROGRAM test;

VAR
	i: INTEGER;

PROCEDURE add(a, b: INTEGER) : INTEGER;
	RETURN a + b
END add;

PROCEDURE returnglobal(a, b, c: INTEGER) : INTEGER;
	VAR
		buf: INTEGER;
	BEGIN
		buf := add(a, b);
		buf := buf + c
	RETURN i
END returnglobal;


BEGIN
	i := 262;
	WRITE returnglobal(4, 5, 6)

END test.